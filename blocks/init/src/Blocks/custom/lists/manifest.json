{
	"$schema": "https://raw.githubusercontent.com/infinum/eightshift-frontend-libs/develop/schemas/block.json",
	"blockName": "lists",
	"title": "List",
	"description" : "lists block with custom settings.",
	"category": "eightshift",
	"icon": {
		"src": "es-lists"
	},
	"keywords": [
		"lists",
		"btn",
		"lists"
	],
	"example": {
		"attributes": {
			"listsAlign": "left"
		}
	},
	"attributes": {
		"listsAlign": {
			"type": "string",
			"default": "left"
		}
	},
	"options": {
		"listsAlign": [
			{
				"label": "Left",
				"value": "left",
				"icon": "textAlignLeft"
			},
			{
				"label": "Center",
				"value": "center",
				"icon": "textAlignCenter"
			},
			{
				"label": "Right",
				"value": "right",
				"icon": "textAlignRight"
			}
		]
	},
	"components": {
		"lists": "lists"
	},
	"variables": {
		"listsAlign": [
			{
				"variable": {
					"block-lists-align": "%value%"
				}
			}
		]
	}
}
