	"namespace": "eightshift-boilerplate",
	"background": "#FBF9FF",
	"foreground": "#9973E3",
	"globalVariables": {
		"customBlocksName": "eightshift-block",
		"maxCols": 12,
		"baseFont": "Inter",
		"zIndex": {
			"header": 100,
			"drawer": 99,
			"overlay": 98
		},
		"breakpoints": {
			"mobile": 480,
			"tablet": 960,
			"desktop": 1920,
			"large": 1921
