{
	"$schema": "https://raw.githubusercontent.com/infinum/eightshift-frontend-libs/develop/schemas/component.json",
	"componentName": "wrapper",
	"title": "Wrapper",
	"componentClass": "wrapper",
	"attributes": {
		"wrapperDisable": {
			"type": "boolean",
			"default": false
		},

		"wrapperLayoutUse": {
			"type": "boolean",
			"default": true
		},

		"wrapperIdShow": {
			"type": "boolean",
			"default": true
		},
		"wrapperId": {
			"type": "string"
		},

		"wrapperAnchorIdShow": {
			"type": "boolean",
			"default": true
		},
		"wrapperAnchorId": {
			"type": "string"
		},

		"wrapperBackgroundColorShow": {
			"type": "boolean",
			"default": true
		},
		"wrapperBackgroundColor": {
			"type": "string"
		},

		"wrapperStartShow": {
			"type": "boolean",
			"default": true
		},
		"wrapperStartLarge": {
			"type": "integer",
			"default": 1
		},
		"wrapperStartDesktop": {
			"type": "integer"
		},
		"wrapperStartTablet": {
			"type": "integer"
		},
		"wrapperStartMobile": {
			"type": "integer"
		},

		"wrapperEndShow": {
			"type": "boolean",
			"default": true
		},
		"wrapperEndLarge": {
			"type": "integer",
			"default": 12
		},
		"wrapperEndDesktop": {
			"type": "integer"
		},
		"wrapperEndTablet": {
			"type": "integer"
		},
		"wrapperEndMobile": {
			"type": "integer"
		},

		"wrapperSpacingTopShow": {
			"type": "boolean",
			"default": true
		},
		"wrapperSpacingTopLarge": {
			"type": "integer"
		},
		"wrapperSpacingTopDesktop": {
			"type": "integer"
		},
		"wrapperSpacingTopTablet": {
			"type": "integer"
		},
		"wrapperSpacingTopMobile": {
			"type": "integer"
		},

		"wrapperSpacingBottomShow": {
			"type": "boolean",
			"default": true
		},
		"wrapperSpacingBottomLarge": {
			"type": "integer",
			"default": 40
		},
		"wrapperSpacingBottomDesktop": {
			"type": "integer"
		},
		"wrapperSpacingBottomTablet": {
			"type": "integer"
		},
		"wrapperSpacingBottomMobile": {
			"type": "integer"
		},

		"wrapperSpacingTopInShow": {
			"type": "boolean",
			"default": true
		},
		"wrapperSpacingTopInLarge": {
			"type": "integer"
		},
		"wrapperSpacingTopInDesktop": {
			"type": "integer"
		},
		"wrapperSpacingTopInTablet": {
			"type": "integer"
		},
		"wrapperSpacingTopInMobile": {
			"type": "integer"
		},

		"wrapperSpacingBottomInShow": {
			"type": "boolean",
			"default": true
		},
		"wrapperSpacingBottomInLarge": {
			"type": "integer"
		},
		"wrapperSpacingBottomInDesktop": {
			"type": "integer"
		},
		"wrapperSpacingBottomInTablet": {
			"type": "integer"
		},
		"wrapperSpacingBottomInMobile": {
			"type": "integer"
		},

		"wrapperDividerTopShow": {
			"type": "boolean",
			"default": true
		},
		"wrapperDividerTopLarge": {
			"type": "boolean"
		},
		"wrapperDividerTopDesktop": {
			"type": "boolean"
		},
		"wrapperDividerTopTablet": {
			"type": "boolean"
		},
		"wrapperDividerTopMobile": {
			"type": "boolean"
		},

		"wrapperDividerBottomShow": {
			"type": "boolean",
			"default": true
		},
		"wrapperDividerBottomLarge": {
			"type": "boolean"
		},
		"wrapperDividerBottomDesktop": {
			"type": "boolean"
		},
		"wrapperDividerBottomTablet": {
			"type": "boolean"
		},
		"wrapperDividerBottomMobile": {
			"type": "boolean"
		}
	},
	"responsiveAttributes": {
		"wrapperStart": {
			"large": "wrapperStartLarge",
			"desktop": "wrapperStartDesktop",
			"tablet": "wrapperStartTablet",
			"mobile": "wrapperStartMobile"
		},
		"wrapperEnd": {
			"large": "wrapperEndLarge",
			"desktop": "wrapperEndDesktop",
			"tablet": "wrapperEndTablet",
			"mobile": "wrapperEndMobile"
		},
		"wrapperSpacingTop": {
			"large": "wrapperSpacingTopLarge",
			"desktop": "wrapperSpacingTopDesktop",
			"tablet": "wrapperSpacingTopTablet",
			"mobile": "wrapperSpacingTopMobile"
		},
		"wrapperSpacingBottom": {
			"large": "wrapperSpacingBottomLarge",
			"desktop": "wrapperSpacingBottomDesktop",
			"tablet": "wrapperSpacingBottomTablet",
			"mobile": "wrapperSpacingBottomMobile"
		},
		"wrapperSpacingTopIn": {
			"large": "wrapperSpacingTopInLarge",
			"desktop": "wrapperSpacingTopInDesktop",
			"tablet": "wrapperSpacingTopInTablet",
			"mobile": "wrapperSpacingTopInMobile"
		},
		"wrapperSpacingBottomIn": {
			"large": "wrapperSpacingBottomInLarge",
			"desktop": "wrapperSpacingBottomInDesktop",
			"tablet": "wrapperSpacingBottomInTablet",
			"mobile": "wrapperSpacingBottomInMobile"
		},
		"wrapperDividerTop": {
			"large": "wrapperDividerTopLarge",
			"desktop": "wrapperDividerTopDesktop",
			"tablet": "wrapperDividerTopTablet",
			"mobile": "wrapperDividerTopMobile"
		},
		"wrapperDividerBottom": {
			"large": "wrapperDividerBottomLarge",
			"desktop": "wrapperDividerBottomDesktop",
			"tablet": "wrapperDividerBottomTablet",
			"mobile": "wrapperDividerBottomMobile"
		}
	},
	"options": {
		"wrapperBackgroundColor": [
			"primary",
			"black",
			"white"
		],
		"wrapperSpacing": {
			"min": -300,
			"max": 300,
			"step": 10
		},
		"wrapperSpacingInner": {
			"min": 0,
			"max": 300,
			"step": 10
		},
		"breakpoints": [
			"large",
			"desktop",
			"tablet",
			"mobile"
		]
	},
	"variables": {
		"wrapperStart": [
			{
				"inverse": true,
				"variable": {
					"wrapper-grid-column-start": "%value%"
				}
			}
		],
		"wrapperEnd": [
			{
				"inverse": true,
				"variable": {
					"wrapper-grid-column-end": "%value%"
				}
			}
		],
		"wrapperSpacingTop": [
			{
				"inverse": true,
				"variable": {
					"wrapper-spacing-top": "calc(var(--base-font-size) * calc(%value%rem / 10))"
				}
			}
		],
		"wrapperSpacingBottom": [
			{
				"inverse": true,
				"variable": {
					"wrapper-spacing-bottom": "calc(var(--base-font-size) * calc(%value%rem / 10))"
				}
			}
		],
		"wrapperSpacingTopIn": [
			{
				"inverse": true,
				"variable": {
					"wrapper-spacing-top-in": "calc(var(--base-font-size) * calc(%value%rem / 10))"
				}
			}
		],
		"wrapperSpacingBottomIn": [
			{
				"inverse": true,
				"variable": {
					"wrapper-spacing-bottom-in": "calc(var(--base-font-size) * calc(%value%rem / 10))"
				}
			}
		],
		"wrapperDividerBottom": {
			"true": [
				{
					"inverse": true,
					"variable": {
						"wrapper-divider-bottom": "1px solid var(--global-colors-primary)"
					}
				}
			]
		},
		"wrapperDividerTop": {
			"true": [
				{
					"inverse": true,
					"variable": {
						"wrapper-divider-top": "1px solid var(--global-colors-primary)"
					}
				}
			]
		},
		"wrapperBackgroundColor": [
			{
				"inverse": true,
				"variable": {
					"wrapper-background-color": "var(--global-colors-%value%)"
				}
			}
		]
	}
}
